    "apca-w3": "^0.1.9",
    "chromatic": "^13.3.4",
    "css-color-converter": "^2.0.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
